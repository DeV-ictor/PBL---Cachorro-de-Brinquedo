module dis7segdecspd (
	input A, B,
	output F_ade, F_cf, F_b, F_g, F_p
);

	wire A_not, B_not;
	
	not (A_not, A);
	not (B_not, B);
	
	// Saída para segmentos A, D, E

	and (F_ade, A, B_not);
	
	// Saída para segmentos C, F

	and (F_cf, A_not, B);

	// Saída para segmentos G

	and (F_g, A_not, B_not);
	
	// Saída para segmentos B

	and (F_b, 0b'0);
	
	// Saída para segmentos P

	and (F_b, 0b'1);
